<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Empleados</servlet-name>
        <servlet-class>Control.Empleados</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SvEmpleados</servlet-name>
        <servlet-class>Control.SvEmpleados</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SvPlatos</servlet-name>
        <servlet-class>Control.SvPlatos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SvMesas</servlet-name>
        <servlet-class>Control.SvMesas</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SvLogin</servlet-name>
        <servlet-class>Control.SvLogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Empleados</servlet-name>
        <url-pattern>/Empleados</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SvEmpleados</servlet-name>
        <url-pattern>/SvEmpleados</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SvPlatos</servlet-name>
        <url-pattern>/SvPlatos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SvMesas</servlet-name>
        <url-pattern>/SvMesas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SvLogin</servlet-name>
        <url-pattern>/SvLogin</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
